{
  "continent": "",            // one of "africa", "antartica" "asia", "australia", "europe", "north america", "south america"
  "country": "",              // ISO 3166-1 alpha-3 country code
  "region": "",               // ISO 3166-2 region code, if available. If not, whatever local postal codes
  "timezone": "",             // IANA time zone code (e.g. "America/Los_Angeles")
  "name": "",                 // Human-friendly name of station
  "id": "",                   // Unique station ID for this database only, usually a slug of the name
  "disclamers": "",           // Any disclamers about using this data
  "restriction": "",          // Any restrictions on this data's use
  "primary_station": "",      // True if this is a station with known harmonics, false if it is a secondary station
  "latitude": 1,              // Latitude, in decimal degrees
  "longitude": 1,              // Longitude, in decimal degrees
  "source": {                 // Where this data came from
    "name": "",               // name of the source, i.e. "NOAA"
    "id": "",                 //The source's station ID
    "published_harmonics": "", // Are the harmonics from published harmonic data? False means harmonics were computed
    "license": "",            // Short description of any terms or conditions on this data
    "url": "",                // URL to this station's data or information in the source's website
    "source_url": ""          // URL to the source's homepage
  },
  "harmonic_constituents": [  // An array of harmonic constituent objects. Leave as empty array if this is a secondary station
    {
      "name": "",             // Harmonic Constituent code (i.e. "M2") - should be upper-case,
      "description": "",      // Optional - name of constituent
      "amplitude": 1,         // Amplitude
      "phase_GMT": 1,         // Phase for GMT timezone
      "phase_local": 1,       // Phase for local timezone
      "speed": 1              // Angular speed
    }
  ],
  "secondary_offset" : {        //For secondary stations, leave an empty object if this is a primary station
      "reference_station": "",  // Station ID (the database, not the source) of the reference station
      "height_offset": {        // High and low-tide water level offsets
        "high": 1,
        "low": 1
      },
      "time_offset": {          // High and low-tide water level offsets
        "high": 1,
        "low": 1
      }
  }     
}


